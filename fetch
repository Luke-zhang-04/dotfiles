#! /bin/bash
# Fetch files and cp

mkdir -pv ./.config/i3
mkdir -pv ./.devilspie
mkdir -pv ./usr/share/conky

cp -rv ~/.Xresources ./.Xresources
cp -rv ~/.config/i3/ ./.config/
cp -rv ~/.config/fish ./.config
cp -rv ~/.config/omf ./.config
cp -rv /usr/share/conky/ ./
cp -v ~/.gitconfig ./.gitconfig
cp -v ~/.bashrc ./.bashrc
cp -v ~/.devilspie/vscode_transparent.ds ./.devilspie/vscode_transparent.ds
cp -v ~/.ombrc ./.ombrc
cp -v ~/.xinitrc ./.xinitrc
cp -v ~/.Xmodmap ./.Xmodmap
cp -v ~/.zshrc ./.zshrc
cp -v ~/.config/picom.conf ./.config/picom.conf
